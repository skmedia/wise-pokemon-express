{
  "name": "wise-pokemon-express",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "load-pokemons-from-file": "npx ts-node load-pokemons-from-file.ts",
    "load-pokemon-from-api": "npx ts-node load-pokemon-from-api.ts",
    "init-db": "npx prisma generate && npx prisma db push --force-reset",
    "lint": "npx eslint ./src",
    "test": "npm rum test:unit && npm run test:integration",
    "test:unit": "dotenv -e .env.test jest -i",
    "test:integration": "dotenv -e .env.test -- npx prisma generate && dotenv -e .env.test -- npx prisma db push --force-reset && dotenv -e .env.test jest -i"
  },
  "keywords": [],
  "author": "",
  "module": "es2022",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "request": "^2.88.2",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express-validator": "^6.14.2",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "nodemon": "^2.0.19",
    "prisma": "^4.2.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}